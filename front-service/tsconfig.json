{
  "compilerOptions": {
    "target": "esnext",                                 
    "module": "esnext",                                
    "rootDir": "./",                              
    "moduleResolution": "node",                     
    "baseUrl": "./",                                  
    "resolveJsonModule": true,                     
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true,
    "jsx": "react-jsx"                                
  }
}
